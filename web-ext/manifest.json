{
  "manifest_version": 2,
  "name": "Auto Bookmark History",
  "version": "0.1.1",
  "description": "automatically bookmark pages that match a set URL",
  "background": {
    "scripts": ["out/bg.js"],
    "type": "module"
  },
  "action": {
  },
  "options_ui": {
    "page": "pages/options/index.html"
  },
  "icons": {
    "48": "icons/icon.png"
  },
  "permissions": [
    "storage",
    "bookmarks",
    "history"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}
