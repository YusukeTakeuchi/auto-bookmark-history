{
  "manifest_version": 3,
  "name": "Auto Bookmark History",
  "version": "0.1.1",
  "description": "automatically bookmark pages that match a set URL",
  "background": {
    "scripts": ["out/bg.js"],
    "type": "module"
  },
  "action": {
  },
  "icons": {
    "48": "icons/icon.png"
  },
  "permissions": [
    "storage",
    "bookmarks",
    "history"
  ]
}
